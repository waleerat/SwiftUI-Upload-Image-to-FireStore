# platform :ios, '9.0'

target 'SwiftUI-Upload-Image-to-FireStore' do
  # Comment the next line if you don't want to use dynamic frameworks
  use_frameworks!

  # Pods for SwiftUI-Upload-Image-to-FireStore
  pod 'Firebase'
